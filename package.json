{
  "name": "tquelch.com",
  "version": "1.0.0",
  "description": "* tquelch.com",
  "main": "index.js",
  "scripts": {
    "build-fontawesome": "mkdir -p assets/css/fontawesome && cp node_modules/@fortawesome/fontawesome-free/scss/* $_",
    "build-normalize": "mkdir -p assets/css/normalize && cp -r node_modules/normalize.css/* $_",
    "build-katex": "mkdir -p static/katex && cp -r node_modules/katex/dist/* $_",
    "build": "npm run build-fontawesome && npm run build-normalize && npm run build-katex"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimQuelch/tquelch.com.git"
  },
  "author": "Tim Quelch",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TimQuelch/tquelch.com/issues"
  },
  "homepage": "https://github.com/TimQuelch/tquelch.com#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "katex": "^0.15.2",
    "normalize.css": "^8.0.1"
  }
}
